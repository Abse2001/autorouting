{
  "name": "autorouting-dataset",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "nodemon --watch frontend --watch module --ext ts,tsx frontend --exec 'bun run build-and-serve'",
    "build": "vite build",
    "build-and-serve": "npm run build && bun module/cli.ts server start",
    "format": "biome format --write .",
    "download-server": "wget --mirror --convert-links --level=100 --adjust-extension --page-requisites --no-parent -P ./static-server http://localhost:3000"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@tscircuit/pcb-viewer": "^1.4.4",
    "@tscircuit/props": "^0.0.26",
    "@tscircuit/soup": "^0.0.39",
    "@tscircuit/soup-util": "^0.0.13",
    "@types/bun": "latest",
    "@types/mersenne-twister": "^1.1.7",
    "@types/node": "^20.14.11",
    "@types/pathfinding": "^0.0.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "pathfinding": "^0.4.18",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-query": "^3.39.3",
    "vite": "^5.3.4",
    "vite-plugin-singlefile": "^2.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@tscircuit/checks": "0.0.23",
    "commander": "^12.1.0",
    "kleur": "^4.1.5",
    "mersenne-twister": "^1.1.0",
    "tscircuit": "^0.0.246"
  }
}
